<Window x:Class="Self_Learning_WPF.Canvas_WPF"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Canvas_WPF" Height="450" Width="800">
    <Canvas>
        <Button Panel.ZIndex="1" Click="Button_Click" Content="1" Width="100" Height="100" FontSize="50" Canvas.Top="10"/>
        <Button x:Name="btn2" Content="2" Panel.ZIndex="0" Width="100" Height="100" FontSize="50" Canvas.Bottom="10"></Button>
        <Ellipse Fill="Aqua" Width="100" Height="100" />
        <Rectangle Fill="Pink" Width="100" Height="200"></Rectangle>
    </Canvas>
    
</Window>
